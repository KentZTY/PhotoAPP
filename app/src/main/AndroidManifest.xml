<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yang.myphoto4">

    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:configChanges="orientation|keyboardHidden" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Menu"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Display_Image"
            android:label="@string/title_activity_display_image"
            android:parentActivityName=".Menu">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Menu" />
        </activity>
        <activity
            android:name=".ShareImageActivity"
            android:label="@string/title_activity_share_image"
            android:parentActivityName=".Display_Image">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Display_Image" />
        </activity>
        <activity
            android:name=".Selector_Sticker"
            android:label="@string/title_activity_share_image"
            android:parentActivityName=".Display_Image">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Display_Image" />
        </activity>
        <activity
            android:name=".Selector_Border"
            android:label="@string/title_activity_share_image"
            android:parentActivityName=".Display_Image">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Display_Image" />
        </activity>
        <activity
            android:name=".Help"
            android:label="@string/title_activity_share_image"
            android:parentActivityName=".Display_Image">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Display_Image" />
        </activity>
        <activity
            android:name=".Sticker_Login"
            android:label="@string/title_activity_share_image"
            android:parentActivityName=".Menu">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Display_Image" />
        </activity>
        <activity
            android:name=".Sticker_Register"
            android:label="@string/title_activity_share_image"
            android:parentActivityName=".Sticker_Login">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Display_Image" />
        </activity>
        <activity
            android:name=".Sticker_FileSync"
            android:label="@string/title_activity_share_image"
            android:parentActivityName=".Sticker_Login">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Display_Image" />
        </activity>
        <activity
            android:name=".Iris"
            android:label="@string/title_activity_iris"
            android:parentActivityName=".Menu">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Menu" />
        </activity>
    </application>

</manifest>
